<h1 class="page-header text-center">Here is your custom dictionary!</h1>
<% if @words.empty? %>
  <h3>Looks like you haven't added any words yet!</h3>
<% end %>
<ul>
  <% @words.each() do |word| %>
  <li><a href="/word/<%= word.id %>"> <%= word.word() %></a></li>
  <% end %>
</ul>

<a href="/add_word">Add a word to dictionary </a>
